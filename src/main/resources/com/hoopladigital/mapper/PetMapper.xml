<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hoopladigital.mapper.PetMapper">

	<select id="getPetList" resultType="Pet">
		select
			id,
			personId,
			name
		from Pet
		order by personId, id
	</select>

</mapper>
